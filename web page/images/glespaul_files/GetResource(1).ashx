.horizontalmenu {
	font: 12px/18px "Lucida Grande", "Lucida Sans Unicode", Arial, Verdana, sans-serif; background-color: #fff; color: #333; 
	text-align: center;
	background: #fff;
	text-align: center;
	height: 40px;
	margin-top: 0px !important;
	margin-bottom: 0;
	font-size:12px;
	border-top: 1px solid #c1c1c1;
	border-bottom: 1px solid #c1c1c1;
	float: left;
	width: 992px;
	padding: 0 0px 0px;
	margin-left: 0;
}

.verticalmenu {
	background: none;
}

/*#Top menu styles/Horizontal#*/

#ctl00_cmsmenu_CMSMenu1_table,
#cmsmenu_CMSMenu1_table,
#ctl00_cmsmenu_menuElem_table {
	float: left;
}

.horizontalCMSMenu {
	color: #333333;
	margin: 10px auto auto;
	float: left;
	width: 775px;
}

.horizontalsubCMSMenu {
	color: #333;
	font-size: 12px;
	margin-top: 10px;
	min-width: 173px;
	max-width: 173px;
	border-right: 1px solid #c1c1c1;
	border-left: 1px solid #c1c1c1;
	border-bottom: 1px solid #c1c1c1;
}

.horizontalsubCMSMenu tr {
	background-color: #fff;
}

.horizontalsubCMSMenuItem {
	white-space: nowrap;
	display: block;
	padding: 3px 10px 10px 3px;
	background-color: #fff;
	margin: 0 0px 0px;

}

.horizontalCMSMenuItemMouseOver,
.horizontalCMSMenuHighlightedMenuItem, 
.horizontalCMSMenuHighlightedMenuItemMouseOver,
.horizontalCMSMenuHighlightedMenuItemMouseDown {
	height: 30px;
	color: #000;
/*	background: #e6e8e8; */
	line-height: 30px;
}

.horizontalsubCMSMenuItem,
.horizontalsubCMSMenuItemMouseOver,
.horizontalsubCMSMenuItemMouseDown,
.horizontalsubCMSMenuHighlightedMenuItem,
.horizontalsubCMSMenuHighlightedMenuItemMouseOver, 
.horizontalsubCMSMenuHighlightedMenuItemMouseDown {
	white-space: nowrap;
	display: block;
	padding: 5px 10px 5px 10px;
	min-width: 153px;
	text-align: left;
	color: #454545;

}

.horizontalsubCMSMenuHighlightedMenuItem,
.horizontalsubCMSMenuHighlightedMenuItemMouseOver, 
.horizontalsubCMSMenuHighlightedMenuItemMouseDown,
.horizontalsubCMSMenuItemMouseOver {
	background: #e6e8e8;
	padding: 5px 10px 5px 10px;
	min-width: 153px;
	text-align: left;
}

.horizontalCMSMenuItem, 
.horizontalCMSMenuItemMouseOver, 
.horizontalCMSMenuItemMouseDown, 
.horizontalCMSMenuHighlightedMenuItem, 
.horizontalCMSMenuHighlightedMenuItemMouseOver, 
.horizontalCMSMenuHighlightedMenuItemMouseDown, 
.horizontalCMSMenuItemMouseUp, 
.horizontalCMSMenuHighlightedMenuItemMouseUp {
	font-size: 12px;
	font-weight: bold;
	color: #787878;
	text-transform: uppercase;
	text-align: center;
	margin: 0;
	float: left;
	line-height: 21px;
	height: 20px;
	border-right: 1px solid #c1c1c1;
	padding-bottom: 0px;
	padding-left: 10px;
	padding-right: 10px;
}

#ctl00_Panel1 #ctl00_txtSearch,
input#txtSearch {
	margin: 0px;
	position: relative;
	width: 123px;
	height: 15px;
	padding: 12px 10px 12px 29px;
	top: -35px;
	left: 416px;
	background: url(http://images.gibson.com.s3.amazonaws.com/Site-Images/Global/TopNav/TopNav-Bg-SearchBoxTab.gif) no-repeat;
	border:  0px;
	float: none;
}

input#txtSearch {

}

.TopNav-txtSearch {
	background: #fff url(http://images.gibson.com/Site-Images/Page-Elements/Header-SearchSpyglass.png) no-repeat 2px;
	margin-top: 8px;
	float: right;
	margin-right: 8px;
	width: 190px;
	height: 20px;
	text-indent: 20px;
	text-transform: uppercase;
	line-height: 20px;
}


