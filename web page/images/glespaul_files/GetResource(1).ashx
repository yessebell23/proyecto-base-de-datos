/* @group Brand Navigation */

menu, menu ul {
	list-style: none;
	padding: 0;
	width: 790px;
	float: left;
	margin-top: 0;
}

menu li {
	display: inline;
	position: relative;
	margin-right: -4px;
	margin-bottom: 0;
}

menu li div {
	display: none;
	width: 300px;
	float: left;
}

menu li div img {
	height: 100px;
	width: 100px;
	float: left;
}

menu li div ul img {
	display:none
}

menu li:hover div, menu li.active div {
	display: block;
}

#header menu li a span {
	text-decoration: none;
	border-style: none;
	border-width: 0;
}

#header menu li a {
	text-decoration: none;
}

