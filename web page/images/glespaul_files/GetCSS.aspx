/* Gradient */
    background-image: linear-gradient(bottom, rgb(230,230,230) 30%, rgb(247,247,247) 61%);
	background-image: -o-linear-gradient(bottom, rgb(230,230,230) 30%, rgb(247,247,247) 61%);
	background-image: -moz-linear-gradient(bottom, rgb(230,230,230) 30%, rgb(247,247,247) 61%);
	background-image: -webkit-linear-gradient(bottom, rgb(230,230,230) 30%, rgb(247,247,247) 61%);
	background-image: -ms-linear-gradient(bottom, rgb(230,230,230) 30%, rgb(247,247,247) 61%);
	background-image: -webkit-gradient(
        linear,
        left bottom,
        left top,
        color-stop(0.3, rgb(230,230,230)),
        color-stop(0.61, rgb(247,247,247))
    );
/*

End Gradient */
}

.infiniteCarousel3 .wrapper {
	/

* width - (.wrapper margin-left + .wrapper margin-right) */
    overflow: hidden;
	min-height: 10em;
	position: absolute;
	width: 666px;
	height: 213px;
}

.infiniteCarousel3 ul a img {
	border: 0 solid #000;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}

.infiniteCarousel3 .wrapper ul {
	width: 9999px;
	list-style-image: none;
	list-style-position: outside;
	list-style-type: none;
	margin: 0;
	padding: 0;
	position: absolute;
	top: 0;
}

.infiniteCarousel3 ul li {
	width: 666px;
	display: block;
	float: left;
	height: 200px;
	text-align: center;
}

.infiniteCarousel3 ul li a img {
	display: block;
	border-style: none;
	margin-left: 33px;
}

.infiniteCarousel3 .arrow {
	display: block;
	height: 36px;
	width: 37px;
	text-indent: -999px;
	position: absolute;
	top: 37px;
	cursor: pointer;
}

.infiniteCarousel3 .forward {
	background-position: 0 0;
	right: 0;
}

.infiniteCarousel3 .back {
	background-position: 0 -72px;
	left: 0;
}

.infiniteCarousel3 .forward:hover {
	background-position: 0 -36px;
}

.infiniteCarousel3 .back:hover {
	background-position: 0 -108px;
}

